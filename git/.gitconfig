[user]
	name = tvergara
	email = tomvergara@uc.cl

[alias]
	co = checkout
	cm = commit
	st = status
	br = branch
	ca = commit --amend
	rb = rebase
	aa = add --all
	wip = commit -a -m "wip" --no-verify
	dewip = reset HEAD~1
	rb-last = "!f() { git rebase -i -r HEAD~$1; }; f"
	pf = push -f
	p = push
	pl = pull

[push]
  default = upstream
